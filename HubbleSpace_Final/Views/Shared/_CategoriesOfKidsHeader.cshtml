@inject HubbleSpace_Final.Entities.MyDbContext context

<li class="nav-item dropdown menu-sidebar">
    <a asp-action="Categories" asp-route-Object="Kids" data-toggle="dropdown" data-hover="dropdown" data-delay="200" class="dropdown-toggle nav-link">Kids<b class="caret"></b></a>
    <ul class="dropdown-menu megamenu">
        @{
            var itemObjects = context.Category.Where(c => c.Object == "Kids").Distinct();
            foreach (var i in itemObjects)
            {
                <li class="nav-item"><a asp-controller="Home" asp-action="Categories" asp-route-Object="@i.Object" asp-route-name="@i.Category_Name" class="nav-link-custom">@i.Category_Name</a></li>
            }

        }
    </ul>
</li>